snippet main
int main(int argc, char *argv[]) {
	$1
	return 0;
}
endsnippet

snippet mainn
int main(void) {
	$1
	return 0;
}
endsnippet

snippet fori
for (int i = 0; i < $1; i++) {
	$2
}
endsnippet

snippet for
for (int $1 = 0; $1 < $2; $1++) {
	$3
}
endsnippet

snippet if
if ($1) {
	$2
}
endsnippet

snippet pf
printf($1)
endsnippet

snippet fun
$1($2) {
	$3
}
endsnippet

snippet i<
#include <$1.h>
endsnippet

snippet i"
#include "$1.h"
endsnippet
